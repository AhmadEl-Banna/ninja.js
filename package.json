{
  "name": "react-interview-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start --env=development",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --no-stash",
      "pre-push": "yarn test:ci"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,css}": "prettier --write",
    "./src/**/*.{js,ts,tsx}": "eslint"
  },
  "dependencies": {
    "immer": "^7.0.7",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.5",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react-dom": "^16.9.8",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/lodash": "^4.14.160",
    "@types/react": "^16.9.47",
    "@types/react-redux": "^7.1.9",
    "@welldone-software/why-did-you-render": "^5.0.0-alpha.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.12",
    "prettier": "^2.1.0",
    "typescript": "^4.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
  
}
